            <strong>Patent number</strong>
            <input type="text" id="patentNumber" class="form-control">
        </div>
        <div class="col-12 mt-2 mb-3">
            <strong>Application number</strong>
            <input type="text" id="patentApplicationNumber" class="form-control">
