<%- include('../../bootstrap') %>

<div class="container-fluid">
    <div class="row">
        <div class="col-12 mt-2 mb-3">
            <strong>Title</strong>
            <input type="text" class="form-control" id="patentTitle">
        </div>
        <div class="col-12 mt-2 mb-3">
            <strong>Inventors (For example: Patterson, David, Lamport, Leslie)</strong>
            <input type="text" class="form-control" id="patentInventors">
        </div>
        <div class="col-12 mt-2 mb-3">
            <strong>Keywords (For example: a, b, c)</strong>
            <input type="text" class="form-control" id="patentKeyword">
        </div>
        <div class="col-12 mt-2 mb-3">
            <strong>Abstract / Description</strong>
            <textarea name="patentAbstract" class="form-control" id="patentAbstract" rows="6"></textarea>
        </div>
        <div class="col-12 mt-2 mb-3">
            <strong>Publication date (For example: 2019, 2019/12 or 2019/10/31)</strong>
            <input type="text" class="form-control" id="patentDate">
        </div>
        <div class="col-12 mt-2 mb-3">
            <strong>Patent Office</strong>
            <select class="custom-select custom-select" id="patentOffice">
